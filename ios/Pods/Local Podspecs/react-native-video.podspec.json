{
  "name": "react-native-video",
  "version": "6.2.0",
  "summary": "A <Video /> element for react-native",
  "description": "A <Video /> element for react-native",
  "license": "MIT",
  "authors": "Community Contributors",
  "homepage": "https://github.com/TheWidlarzGroup/react-native-video",
  "source": {
    "git": "https://github.com/TheWidlarzGroup/react-native-video.git",
    "tag": "v6.2.0"
  },
  "platforms": {
    "ios": "13.0",
    "tvos": "13.0",
    "visionos": "1.0"
  },
  "compiler_flags": "-DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -DFOLLY_CFG_NO_COROUTINES=1 -DFOLLY_HAVE_CLOCK_GETTIME=1 -Wno-comma -Wno-shorten-64-to-32",
  "dependencies": {
    "React-Core": [

    ],
    "RCT-Folly": [
      "2024.01.01.00"
    ],
    "glog": [

    ],
    "React-RCTFabric": [

    ],
    "React-Codegen": [

    ],
    "RCTRequired": [

    ],
    "RCTTypeSafety": [

    ],
    "ReactCommon/turbomodule/bridging": [

    ],
    "ReactCommon/turbomodule/core": [

    ],
    "React-NativeModulesApple": [

    ],
    "Yoga": [

    ],
    "React-Fabric": [

    ],
    "React-graphics": [

    ],
    "React-utils": [

    ],
    "React-featureflags": [

    ],
    "React-debug": [

    ],
    "React-ImageManager": [

    ],
    "React-rendererdebug": [

    ],
    "DoubleConversion": [

    ],
    "hermes-engine": [

    ]
  },
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/boost\" \"$(PODS_ROOT)/Headers/Private/Yoga\"",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20",
    "OTHER_CPLUSPLUSFLAGS": "$(inherited) -DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -DFOLLY_CFG_NO_COROUTINES=1 -DFOLLY_HAVE_CLOCK_GETTIME=1 -Wno-comma -Wno-shorten-64-to-32"
  },
  "default_subspecs": "Video",
  "subspecs": [
    {
      "name": "Video",
      "source_files": "ios/Video/**/*.{h,m,swift}"
    }
  ]
}
